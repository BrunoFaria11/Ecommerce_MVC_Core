<script>
    $(document).ready(function () {
        bsCustomFileInput.init();
    });
    $('[data-mask]').inputmask()

    const Toast = Swal.mixin({
        toast: true,
        position: 'top-end',
        showConfirmButton: false,
        timer: 10000
    });

    async function doAjax(url, IdEntity, IdForm, Table) {
        event.preventDefault();
        debugger;
        $.post(url, { id: IdEntity, model: $(IdForm).serializeFormJSON() }, function (htmlresponse) {
            debugger
            if (!htmlresponse.error) {
                CloseModal();
                Toast.fire({
                    icon: 'success',
                    title: htmlresponse.result,
                    timer: 5000

                })
                debugger;

                if (htmlresponse.id != "" && htmlresponse.id != "0" && htmlresponse.id != null && htmlresponse.id != undefined) {
                    if (Table != "") {
                        TableLaod();
                    }
                    else {
                        window.location.reload();
                    }
                    return htmlresponse.id;
                }
                else {
                    if (Table != "") {
                        TableLaod();
                    }
                    else {
                        window.location.reload();
                    }
                }
            }
            else {
                Toast.fire({
                    icon: 'error',
                    title: htmlresponse.result
                })
            }
        });
    }

  

   

    function CloseModal() {
        $("body").css("padding-right", "0px");
        $("body").removeClass("modal-open");
        $('.modal').removeClass('show');
        $('.modal').data('bs.modal', null);
        $(".modal").css("display", "none");
        $(".modal-backdrop").remove();
    }
</script>